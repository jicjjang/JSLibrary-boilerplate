{
  "name": "jslibrary-boilerplate",
  "version": "0.0.0-semantically-released",
  "description": "javascript library boilerplate",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage:perfect": "istanbul check-coverage --statements 100 --branchs 100 --functions 100 --lines 100",
    "check-coverage": "istanbul check-coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec test/index.test.js",
    "start": "node src/index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jicjjang/JSLibrary-boilerplate.git"
  },
  "keywords": [
    "javascript",
    "library",
    "boilerplate"
  ],
  "author": "junseok Choi <jicjjang12@gmail.com> (http://jicjjang.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jicjjang/JSLibrary-boilerplate/issues"
  },
  "homepage": "https://github.com/jicjjang/JSLibrary-boilerplate#readme",
  "dependencies": {
    "axios": "0.16.1",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov.io": "0.1.6",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "^3.4.1",
    "semantic-release": "^6.3.6"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
